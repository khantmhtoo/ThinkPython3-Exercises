###
Write a function that draws a grid like the following:
+ - - - - + - - - - +
|         |         |
|         |         |
|         |         |
|         |         |
+ - - - - + - - - - +
|         |         |
|         |         |
|         |         |
|         |         |
+ - - - - + - - - - +
###

def do_twice(n):
  n()
  n()

def do_four(n):
  do_twice(n)
  do_twice(n)

def printhead():
  print('+ - - - - + - - - - +')

def printspace():
  print('|         |         |')

def thebox():
  printhead()
  do_four(printspace)

def finishbox():
  do_twice(thebox)
  printhead()

finishbox()

