def is_power(a, b):
    if a % b == 0:
        return True
        if a/b == 0:
            return True
        else:
            return is_power(a/b, b)
    else:
        return False

def prompt_o():
    a = int(input("Enter value for a: "))
    b = int(input("Enter value for b: "))
    return is_power(a, b)

