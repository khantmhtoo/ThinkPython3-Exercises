def gcd(a, b):
    if b == 0:
        return a
    else:
        r = a % b #reminder
        if r == 0:
            return b
        else:
            return gcd(b, r)

def prompt_b():
    a = int(input("Enter value for a: "))
    b = int(input("Enter value for b: "))
    return gcd(a,b)

print(prompt_b())
